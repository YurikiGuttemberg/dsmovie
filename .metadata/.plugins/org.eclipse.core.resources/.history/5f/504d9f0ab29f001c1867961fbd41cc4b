package com.devsuperior.dsmovie.entities;

public class ScorePK {
	private Movie movie;
	private User user;
	private Double value;
	
	public ScorePK() {
	}

	public ScorePK(Movie movie, User user, Double value) {
		super();
		this.movie = movie;
		this.user = user;
		this.value = value;
	}

	public Movie getMovie() {
		return movie;
	}

	public void setMovie(Movie movie) {
		this.movie = movie;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public Double getValue() {
		return value;
	}

	public void setValue(Double value) {
		this.value = value;
	}
	
	
}
